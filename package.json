{
  "name": "editdata",
  "version": "0.0.1",
  "description": "",
  "main": "main.js",
  "bin": {
    "editdata": "cli.js"
  },
  "scripts": {
    "postinstall": "cd node_modules/leveldown && HOME=~/.electron-gyp node-gyp rebuild --target=0.33.6 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "bundle-css": "sheetify assets/deps.css > assets/bundle.css",
    "bundle": "npm run bundle-css",
    "test": "tape tests/*.js | tap-spec",
    "start": "electron .",
    "dev": "npm run bundle && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/editdata/editdata.git"
  },
  "keywords": [
    "data",
    "editor",
    "csv",
    "json",
    "geojson"
  ],
  "author": "sethvincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/editdata/editdata/issues"
  },
  "homepage": "https://github.com/editdata/editdata#readme",
  "dependencies": {
    "csskit": "^1.1.1",
    "data-editor": "editdata/data-editor",
    "data-format": "editdata/data-format",
    "data-grid": "editdata/data-grid",
    "data-map": "editdata/data-map",
    "electron-prebuilt": "^0.33.6",
    "leveldown": "^1.4.1",
    "levelup": "^1.2.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "redux": "^3.0.2",
    "virtual-dom": "^2.1.1",
    "xhr": "^2.1.0",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "sheetify": "^2.0.3",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1"
  }
